{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["ES2015", "dom"],
    "module": "ES2015",
    "types": ["cypress", "node"],
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "skipLibCheck": true
  }
}

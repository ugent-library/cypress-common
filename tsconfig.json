{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "lib": ["ES2015", "DOM"],
    "types": ["cypress", "node"],
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "baseUrl": "."
  },
  "include": ["**/*.ts"]
}
